# Application
APP_NAME="Accounting System"
APP_ENV=production
APP_DEBUG=false
APP_URL=https://your-domain.com

# Database
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=accounting_system_prod
DB_USERNAME=accounting_prod_user
DB_PASSWORD=CHANGE_THIS_SECURE_PASSWORD
DB_ROOT_PASSWORD=CHANGE_THIS_ROOT_PASSWORD

# Redis
REDIS_HOST=redis
REDIS_PASSWORD=CHANGE_THIS_REDIS_PASSWORD
REDIS_PORT=6379

# JWT
JWT_SECRET=CHANGE_THIS_TO_RANDOM_STRING_64_CHARS_OR_MORE
JWT_ALGO=HS256
JWT_EXPIRATION=3600

# Mail
MAIL_MAILER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=tls

# Logging
LOG_CHANNEL=stack
LOG_LEVEL=error
